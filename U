local ultimateOptions = {
	"+5% Rep Speed",   
	"+1 Pet Slot",      
	"+10 Item Capacity",       
	"+1 Daily Spin",
	"Muscle Mind",    
	"Jungle Swift",    
	"Infernal Health",
	"Galaxy Gains", 
	"Demon Damage",
	"Golden Rebirth", 
	"x2 Chest Rewards",
	"x2 Quest Rewards"
}

local selectedUltimates = {}
local autoBuyUltimatesActive = false

local ultimatesMultiDropdown = ultimatesfolder:AddMultiDropdown("Select Ultimates", function(selected_items)
	selectedUltimates = {}
	for ultimateName, _ in pairs(selected_items) do
		selectedUltimates[ultimateName] = true
	end
end)

local function populateUltimatesDropdown()
	for _, ultimate in ipairs(ultimateOptions) do
		ultimatesMultiDropdown:Add(ultimate)
	end
end

populateUltimatesDropdown()

